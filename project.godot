config_version=5

[application]

config/name="2dFarm"
run/main_scene="res://Menu/TitleMenu/TitleMenu.tscn"
config/features=PackedStringArray("4.0", "GL Compatibility")
boot_splash/show_image=false
config/icon="res://icon.svg"

[audio]

buses/default_bus_layout="res://audio_bus_layout.tres"

[autoload]

GameEvent="*res://GameEvent/GameEvent.tscn"
GlobalShader="*res://Shader/Shader.tscn"
Setting="*res://Setting/Setting.tscn"
SceneChanger="*res://SceneChanger/SceneChanger.tscn"
GameManager="*res://GameManager/GameManager.tscn"
Dialogic="*res://addons/dialogic/Other/DialogicGameHandler.gd"
GameDatabase="*res://GameDatabase/GameDatabase.tscn"
SoundManager="*res://SoundManager/SoundManager.tscn"

[debug]

gdscript/warnings/enable=false

[dialogic]

editor/character_preview_mode=0
editor/current_editor=&"Layout"
editor/current_resources={
"Character Editor": "res://dialogic/character/officer_character.dch",
"Timeline Editor": "res://dialogic/Timeline/in_game.dtl"
}
animations/join_default_length=0.5
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/join_default_wait=true
choices/autofocus_first=true
choices/delay=0.2
save/autosave_delay=60.0
text/speed=0.05
text/skippable=true
text/autocontinue_delay=1.0
glossary/default_case_sensitive=true
editor/current_timeline_path="res://dialogic/Timeline/in_game.dtl"
variables={}
editor/timeline_editor_mode=0
editor/last/resources=[]
editor/visual_timeline_editor_sidebar=188.0
extensions_folder="res://addons/dialogic_additions"
history/already_read_history_enabled=false
history/full_history_enabled=false
history/simple_history_enabled=false
translation/enabled=false
translation/original_locale="en"
translation/file_mode=1
translation/translation_folder=""
editor/layout/mode=2
layout/layout_scene="res://dialogic/CustomDefaultLayout/custom_DialogicDefaultLayout.tscn"
layout/export_overrides={}
save/autosave=true
text/autocolor_names=true
layout/mode=1

[display]

window/size/viewport_width=320
window/size/viewport_height=180
window/size/window_width_override=960
window/size/window_height_override=540
window/stretch/mode="canvas_items"

[editor]

naming/scene_name_casing=1

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg")

[gui]

theme/custom="res://theme.tres"
theme/custom_font="res://Font/BoonTookMon-Regular.otf"

